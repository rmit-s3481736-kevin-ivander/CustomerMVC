@model IEnumerable<CustomerMVC.Models.Payment>

@{
    ViewBag.Title = "Index";
}

<h2>Payment</h2>
<br />
<div>Payment Success.</div>
<br />

<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Card Number
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.First_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Last_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardNo)
        </td>
    </tr>
}
</table>
<p class="btn btn-default">@Html.ActionLink("OK", "Index", "Home")</p>
